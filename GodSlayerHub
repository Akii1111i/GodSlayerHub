local TraitsTab = Window:CreateTab("üé≤ Traits", nil)
local Section = TraitsTab:CreateSection("Change Traits Of Your Units Here!")
local Label = TraitsTab:CreateLabel("üíÆ Best to Use in a Private Server üíÆ", nil, Color3.fromRGB(255, 189, 204, 1), true)

local characterName = "SunGod"
local traitValue = 10

-- Input for character name
local NameInput = TraitsTab:CreateInput({
   Name = "Character Name",
   CurrentValue = "",
   PlaceholderText = "Enter Character Name",
   RemoveTextAfterFocusLost = false,
   Flag = "NameInput",
   Callback = function(Text)
       characterName = Text

       local args = {
           [1] = characterName,
           [2] = traitValue
       }

       game:GetService("ReplicatedStorage").Events.Trait:FireServer(unpack(args))
   end,
})

-- Input for trait value
local ValueInput = TraitsTab:CreateInput({
   Name = "Trait Value 1-10 (10 is Ethereal and 1 is Punch",
   CurrentValue = "",
   PlaceholderText = "Enter Trait Number (e.g. 10)",
   RemoveTextAfterFocusLost = false,
   Flag = "ValueInput",
   Callback = function(Text)
       local number = tonumber(Text)
       if number then
           traitValue = number

           local args = {
               [1] = characterName,
               [2] = traitValue
           }

           game:GetService("ReplicatedStorage").Events.Trait:FireServer(unpack(args))
       end
   end,
})


local ItemsTab = Window:CreateTab("ü™ê Items", nil)
local Label = ItemsTab:CreateLabel("üíÆ Give yourself Items here! üíÆ", nil, Color3.fromRGB(255, 189, 204, 1), true)

local itemName = "Gems"
local itemAmount = 1

-- Input for Item Name
local NameInput = ItemsTab:CreateInput({
   Name = "Item Name",
   CurrentValue = "",
   PlaceholderText = "Enter Item Name (e.g. Gems, TraitRR)",
   RemoveTextAfterFocusLost = false,
   Flag = "ItemNameInput",
   Callback = function(Text)
       itemName = Text

       local args = {
           1,
           itemName,
           "94410702314098"
       }

       local event = game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("GiveItem")

       for i = 1, itemAmount do
           event:FireServer(unpack(args))
       end
   end,
})

-- Input for Amount (loop count)
local AmountInput = ItemsTab:CreateInput({
   Name = "Item Amount",
   CurrentValue = "",
   PlaceholderText = "Enter Quantity (e.g. 1, 50, 1000)",
   RemoveTextAfterFocusLost = false,
   Flag = "ItemAmountInput",
   Callback = function(Text)
       local number = tonumber(Text)
       if number then
           itemAmount = number

           local args = {
               1,
               itemName,
               "94410702314098"
           }

           local event = game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("GiveItem")

           for i = 1, itemAmount do
               event:FireServer(unpack(args))
           end
       end
   end,
})

