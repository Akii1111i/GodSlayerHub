   },

   KeySystem = true,
   KeySettings = {
      Title = "GodSlayerHub",
      Subtitle = "Key System",
      Note = "Join Discord for The key.",
      FileName = "GUuhYHGG9GUUghsght3ty7tUOBGBubisgbb",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"GOgasWhbFDhbgfVdBSiwtgNGIHGUIThpnWbyfhs"}
   }
})

