local LevelsTab = Window:CreateTab("üé¥ Levels", nil)
local Label = LevelsTab:CreateLabel("üíÆ Give Levels to your units here! üíÆ", nil, Color3.fromRGB(255, 189, 204, 1), true)

local characterName = "SunGod"
local levelNumber = 8

-- Character name input
local NameInput = LevelsTab:CreateInput({
   Name = "Character Name",
   CurrentValue = "",
   PlaceholderText = "Enter Character Name (e.g. SunGod)",
   RemoveTextAfterFocusLost = false,
   Flag = "CharNameInput",
   Callback = function(Text)
       characterName = Text

       local args = {
           [1] = levelNumber,
           [2] = characterName .. "Level"
       }

       game:GetService("ReplicatedStorage").Events.GiveItem:FireServer(unpack(args))
   end,
})

-- Number input
local NumberInput = LevelsTab:CreateInput({
   Name = "Level Number",
   CurrentValue = "",
   PlaceholderText = "Enter Number (e.g. 8)",
   RemoveTextAfterFocusLost = false,
   Flag = "LevelNumInput",
   Callback = function(Text)
       local number = tonumber(Text)
       if number then
           levelNumber = number

           local args = {
               [1] = levelNumber,
               [2] = characterName .. "Level"
           }

           game:GetService("ReplicatedStorage").Events.GiveItem:FireServer(unpack(args))
       end
   end,
})
